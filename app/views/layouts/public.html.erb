<!DOCTYPE html>
<html>
  <head>
    <title>namespace - public</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'public', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<body>
  <header>
    <div class="header_container">
      <div class="header_logo">
        <%= link_to "LOGO", root_path %>
      </div>
      <div class="header_menu">
        <%= link_to "商品一覧", root_path %>
      </div>
      <div class="header_menu">
        <%= link_to "新規登録", new_end_user_registration_path %>
      </div>
      <div class="header_menu">
        <%= link_to "ログイン", new_end_user_session_path %>
      </div>
      <div class="header_menu">
        <%= link_to "ログアウト", destroy_end_user_session_path , method: :delete %>
      </div>
    </div>
  </header>
<% content_for :content do %>  
  <%= yield %>
<% end %>
<%= render template: "layouts/application" %>
</body>
</html>